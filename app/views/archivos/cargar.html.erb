<div class="row">
  <div class="large-12 columns">
      <nav class="top-bar" data-topbar>
        <ul class="title-area">
          <li class="name">
            <h1>
              <a href="#">
                Pitagoras
              </a>
            </h1>
          </li>
          <li class="toggle-topbar menu-icon"><a href="#"><span>menu</span></a></li>
        </ul>
        <section class="top-bar-section">
          <ul class="left">
            <li><%= link_to 'Subir Archivos', archivos_cargar_path  %></li>
            <li><%= link_to 'Ver Notas', archivos_profesor_notas_path  %></li>
            <li><%= link_to 'Mandar Correo', profesores_correo_masivo_path  %></li>
            <li><%= link_to 'Salir', profesores_logout_path  %></li>
          </ul>
        </section>
      </nav>
    </div>
  </div>
  <br>
  <div class="row">
    <section>
      <div class="column"> 
        <p><h4><%= session['profesor'] %></h4></p>
        <p>Subir archivo: </p>
      </div> 
      <br />
      <div class="column">
        <div class="form-centered">
          <%= form_tag({:controller => "archivos", :action => "subir_archivo"}, :method =>"post", :multipart => "true") do %>
             <input type="file" id="<%= :archivo %>" name="<%= :archivo %>" value="<%= :archivo %>" accept="application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet">
             <%= submit_tag "Subir" %>
          <% end %>
        </div> 
      </div> 
    </section>
  </div>
  <br> 
  <footer class="row">
    <div class="large-12 columns">
      <hr>
      <div class="row">
        <div class="large-6 columns">
          <p>Â© Copyright MJRuby</p>
        </div>
      </div>
    </div>
  </footer>