<div class="row">
  <div class="large-12 columns">
      <nav class="top-bar" data-topbar>
        <ul class="title-area">
          <li class="name">
            <h1>
              <a href="#">
                Pitagoras
              </a>
            </h1>
          </li>
          <li class="toggle-topbar menu-icon"><a href="#"><span>menu</span></a></li>
        </ul>
        <section class="top-bar-section">
          <ul class="left">
            <li><%= link_to 'Notas', archivos_estudiante_notas_path  %></li>
            <li><%= link_to 'Subscripción a correos', suscripciones_inicio_path  %></li>
          </ul>
        </section>
      </nav>
    </div>
  </div>
  <br>
  <div class="row">
    <section>
      <div class="column">
        <div class="form-centered">
          <table>
            <thead>
              <th>Materia</th>
              <th>Nota 1</th>
              <th>Nota 2</th>
              <th>Nota 3</th>
              <th>Final</th>
            </thead>
            <tbody>
              <% for notas in $listaMaterias %>
              <tr>
                <% for datos in notas %>
                  <td><%=datos%></td>
                <% end %>
              </tr>
              <% end %>
            </tbody>
          </table>
        </div> 
      </div> 
    </section>
  </div>
  <br> 
  <footer class="row">
    <div class="large-12 columns">
      <hr>
      <div class="row">
        <div class="large-6 columns">
          <p>© Copyright MJRuby</p>
        </div>
      </div>
    </div>
  </footer>